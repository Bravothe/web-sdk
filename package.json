{
  "name": "evzone-pay-sdk",
  "version": "1.0.7",
  "description": "EVzone Wallet payment form component for React",
  "type": "module",
  "main": "dist/WalletPaymentForm.umd.js",
  "module": "dist/WalletPaymentForm.esm.js",
  "exports": {
    ".": {
      "import": "./dist/WalletPaymentForm.esm.js",
      "require": "./dist/WalletPaymentForm.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wallet",
    "payment",
    "react",
    "evzone"
  ],
  "author": "Allan Bravo",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0 <21",
    "react-dom": ">=18.0.0 <21",
    "react-icons": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-url": "^8.0.2",
    "core-js": "^3.41.0",
    "rollup": "^4.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bravothe/payment-library.git"
  },
  "bugs": {
    "url": "https://github.com/Bravothe/payment-library/issues"
  },
  "homepage": "https://github.com/Bravothe/payment-library#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
